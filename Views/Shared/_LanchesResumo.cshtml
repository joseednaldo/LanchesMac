@model Lanche
<div class="col-sm-4 col-lg-4 col-md-4">
    <div class="img-thumbnail">
        <p><img src="@Model.ImagemUrl" class="img-fluid" alt="" /></p>
    </div>

    <div class="caption">
        <h3 class="pull-righ"> @Model.Preco.ToString("c")</h3>
        <h3>Lanche: @Model.Nome</h3>
        <p>@Model.DescricaoCurta</p>
    </div>



</div>